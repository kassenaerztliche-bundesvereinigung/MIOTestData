{
    "name": "@kbv/miotestdata",
    "version": "1.4.1",
    "description": "Testdata Package for KBV MIOParser and MIOViewer",
    "author": "KBV",
    "keywords": [
        "mio",
        "fhir",
        "kbv",
        "data",
        "test"
    ],
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "prettier": "prettier --write ./**/*.json ./**/*.js ./**/*.ts",
        "lint": "eslint ./**/*.json ./**/*.ts",
        "clean": "rimraf ./dist/data",
        "build": "npm run clean && npm run copy && tsc && npm link",
        "copy": "mkdirp ./dist/data && ncp ./data ./dist/data"
    },
    "jest": {
        "verbose": true,
        "preset": "ts-jest"
    },
    "devDependencies": {
        "@types/jest": "^26.0.23",
        "@types/ncp": "^2.0.4",
        "@types/node": "^14.17.4",
        "@typescript-eslint/eslint-plugin": "^3.10.1",
        "@typescript-eslint/parser": "^3.10.1",
        "eslint": "^7.29.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-prettier": "^3.4.0",
        "jest": "^26.6.3",
        "ncp": "^2.0.0",
        "prettier": "^2.3.2",
        "ts-jest": "^26.5.6",
        "typescript": "^3.9.10"
    },
    "dependencies": {
        "rimraf": "^3.0.2"
    }
}
