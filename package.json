{
    "author": "KBV",
    "name": "@kbv/miotestdata",
    "version": "1.6.0",
    "description": "Testdata Package for KBV MIOParser and MIOViewer",
    "repository": "github:kassenaerztliche-bundesvereinigung/miotestdata",
    "homepage": "https://github.com/kassenaerztliche-bundesvereinigung/miotestdata#readme",
    "bugs": {
        "url": "https://github.com/kassenaerztliche-bundesvereinigung/miotestdata/issues",
        "email": "support.mio@kbv.de"
    },
    "keywords": [
        "mio",
        "fhir",
        "kbv",
        "data",
        "test"
    ],
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "prettier": "prettier --write ./**/*.json ./**/*.js ./**/*.ts",
        "lint": "eslint ./**/*.json ./**/*.ts",
        "clean": "rimraf ./dist/data",
        "build": "npm run clean && npm run copy && tsc && npm link",
        "copy": "mkdirp ./dist/data && ncp ./data ./dist/data"
    },
    "jest": {
        "verbose": true,
        "preset": "ts-jest"
    },
    "dependencies": {
        "mkdirp": "^1.0.4",
        "rimraf": "^3.0.2"
    },
    "devDependencies": {
        "@types/jest": "^27.0.2",
        "@types/mkdirp": "^1.0.2",
        "@types/ncp": "^2.0.5",
        "@types/node": "^16.11.6",
        "@typescript-eslint/eslint-plugin": "^5.2.0",
        "@typescript-eslint/parser": "^5.2.0",
        "eslint": "^8.1.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^27.3.1",
        "ncp": "^2.0.0",
        "prettier": "^2.4.1",
        "ts-jest": "^27.0.7",
        "typescript": "^4.4.4"
    }
}
